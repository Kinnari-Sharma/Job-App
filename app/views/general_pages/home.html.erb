<% if logged_in? %>
  
  <% if current_user.admin? %>
    <%= render 'admin/home'%>
  <% else %>
    <%= render 'shared/jobseeker_home' %>
  <% end %>

<% else %>

  <div class="center jumbotron">
    <h1>Welcome to the Job App</h1>
	  <%= link_to "Register", signup_path, class: "btn btn-lg btn-primary" %>
	</div>

<% end %>


